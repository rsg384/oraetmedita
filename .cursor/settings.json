{
  "cursor.deploy.enabled": true,
  "cursor.deploy.repository": "https://github.com/rsg384/oraetmedita.git",
  "cursor.deploy.branch": "main",
  "cursor.deploy.script": "./cursor-deploy.sh",
  "cursor.deploy.autoCommit": true,
  "cursor.deploy.autoPush": true,
  "cursor.deploy.commitMessage": "üìù [AUTO] Deploy autom√°tico - {timestamp}",
  "cursor.deploy.checkBeforeDeploy": true,
  "cursor.deploy.notifications": true,
  "cursor.deploy.backup": true,
  "cursor.deploy.rollback": true,
  "cursor.deploy.timeout": 30000,
  "cursor.deploy.retries": 3,
  "cursor.deploy.verbose": true,
  "cursor.deploy.colors": true,
  "cursor.deploy.logFile": ".cursor/deploy.log",
  "cursor.deploy.ignoreFiles": [
    ".DS_Store",
    "node_modules/",
    "*.log",
    ".env",
    "*.tmp"
  ],
  "cursor.deploy.requiredFiles": [
    "index.html",
    "dashboard.html",
    "minhas-meditacoes.html",
    "supabase-config.js",
    "chatgpt-api.js",
    "api-config.js"
  ],
  "cursor.deploy.preDeployChecks": [
    "git status",
    "git diff --name-only",
    "ls -la *.html",
    "ls -la *.js"
  ],
  "cursor.deploy.postDeployChecks": [
    "git log --oneline -1",
    "git remote -v",
    "git branch --show-current"
  ],
  "cursor.deploy.errorHandling": {
    "continueOnError": false,
    "retryOnError": true,
    "maxRetries": 3,
    "rollbackOnError": true
  },
  "cursor.deploy.notifications": {
    "success": "‚úÖ Deploy conclu√≠do com sucesso!",
    "error": "‚ùå Erro no deploy",
    "warning": "‚ö†Ô∏è Aviso no deploy",
    "info": "‚ÑπÔ∏è Informa√ß√£o do deploy"
  },
  "cursor.deploy.templates": {
    "commitMessage": "üìù [AUTO] Deploy autom√°tico - {timestamp}\n\n‚ú® Mudan√ßas:\n- {changes}\n\nüõ†Ô∏è Tecnologias:\n- HTML5, CSS3, JavaScript\n- Supabase (PostgreSQL)\n- OpenAI ChatGPT API\n\nüë®‚Äçüíª Autor: Rodrigo Silva Goes",
    "errorMessage": "‚ùå Erro no deploy: {error}",
    "successMessage": "‚úÖ Deploy conclu√≠do: {repository}",
    "warningMessage": "‚ö†Ô∏è Aviso: {warning}"
  },
  "cursor.deploy.hooks": {
    "preDeploy": "./cursor-deploy.sh -c",
    "postDeploy": "./cursor-deploy.sh -s",
    "onError": "echo 'Erro no deploy' && exit 1",
    "onSuccess": "echo 'Deploy bem-sucedido'"
  },
  "cursor.deploy.environment": {
    "GIT_AUTHOR_NAME": "Rodrigo Silva Goes",
    "GIT_AUTHOR_EMAIL": "rodrigo@oraetmedita.com",
    "GIT_COMMITTER_NAME": "Rodrigo Silva Goes",
    "GIT_COMMITTER_EMAIL": "rodrigo@oraetmedita.com"
  },
  "cursor.deploy.security": {
    "checkSensitiveFiles": true,
    "ignoreSensitivePatterns": [
      "*.key",
      "*.pem",
      ".env*",
      "config.json"
    ],
    "validateBeforeDeploy": true
  },
  "cursor.deploy.monitoring": {
    "logDeployments": true,
    "trackPerformance": true,
    "notifyOnCompletion": true,
    "saveDeployHistory": true
  }
} 